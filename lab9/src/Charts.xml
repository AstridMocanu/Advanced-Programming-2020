<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entity.Charts">
        <table name="charts" schema="MusicAlbums" catalog=""/>
        <id-class class="entity.ChartsPK"/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <id name="albumId">
                <column name="album_id" precision="11"/>
            </id>
            <basic name="position">
                <column name="position" precision="11"/>
            </basic>
            <basic name="artistId">
                <column name="artist_id" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
